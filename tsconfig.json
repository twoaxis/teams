{
	"compilerOptions": {
		"target": "ES2015",
		"outDir": "./build",
		"rootDir": "./src",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"moduleResolution": "Node",
		"sourceMap": true
	},
	"exclude": [
		"node_modules"
	]
}
